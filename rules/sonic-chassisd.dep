SPATH       := $($(SONIC_CHASSISD)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-chassisd.mk rules/sonic-chassisd.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_CHASSISD)_CACHE_MODE  := GIT_CONTENT_SHA
$(SONIC_CHASSISD)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_CHASSISD)_DEP_FILES   := $(DEP_FILES)
$(SONIC_CHASSISD)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_CHASSISD)_SMDEP_PATHS := $(SPATH)
